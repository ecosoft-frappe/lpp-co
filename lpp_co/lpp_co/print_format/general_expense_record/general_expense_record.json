 "css": "body {\n    padding: 3mm 0 3mm !important;\n}\n.print-format {\n    line-height: 1.5;\n    margin-left: 5mm;\n    margin-right: 5mm;\n    font-family: \"NotoSansThaiLooped\";\n}\n.print-format, .print-format td, .print-format th {\n    font-size: 12px !important;\n    color: #000000 !important;\n}\n.print-format-preview{\n    padding: 5mm;\n}\n.logo {\n    margin-top: 10px;\n    width: 100px;\n}\n.title {\n    font-size: 18px;\n    font-family: \"NotoSansThai\";\n}\n.title-sale-invoice {\n    font-size: 26px !important;\n    padding-top: 30px;\n    padding-left: 0px;\n}\ntable {\n    width: 100%;\n}\n.table-head {\n    border-top: 1px solid #6D6E71;\n    border-bottom: 1px solid #6D6E71;\n}\n.line-break {\n    border-top: 1px solid #6D6E71;\n}\n.col-1 {\n    width: 5%;\n}\n.col-3 {\n    width: 29%;\n}\n.col-2, .col-4, .col-5, .col-6, .col-7, .col-8 {\n    width: 11%;\n}\n.col-foot-1 {\n    width: 35%;\n}\n.col-foot-2 {\n    width: 25%;\n}\n.col-foot-3 {\n    width: 10%;\n}\n.col-foot-4 {\n    width: 30%;\n}\n.page-break-inside {\n    page-break-inside: avoid;\n}\n.add-border{\n    border: 1px solid black !important;\n}\n.color-thin{\n    color: #6D6E71;\n}",
