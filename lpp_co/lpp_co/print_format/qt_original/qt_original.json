 "doctype": "Print Format",
 "font_size": 14,
 "hide_if_not_default": 0,
 "html": "<style>\n    @font-face {\n        font-family: \"NotoSansThai\";\n        src: url(\"{{ frappe.utils.get_url() }}/assets/lpp_co/fonts/NotoSansThai-Regular.ttf\") format(\"truetype\");\n        font-weight: normal;\n        font-style: normal;\n    }\n    @font-face {\n        font-family: \"NotoSansThaiLooped\";\n        src: url(\"{{ frappe.utils.get_url() }}/assets/lpp_co/fonts/NotoSansThaiLooped-Regular.ttf\") format(\"truetype\");\n        font-weight: normal;\n        font-style: normal;\n    }\n</style>\n\n<!-- Get value from doctype -->\n{% set company = frappe.db.get_value(\n    \"Company\",\n    {\"name\": doc.company},\n    [\"company_logo\", \"company_name\", \"custom_company_name_en\", \"custom_address_en\"],\n    as_dict=True) %}\n{% set customer_address = frappe.db.get_value(\n    \"Address\",\n    {\"name\": (\"=\", doc.customer_address)},\n    [\"address_line1\", \"address_line2\", \"city\", \"county\", \"state\", \"pincode\"],\n    as_dict=True) %}\n{% set owner = frappe.db.get_value(\n    \"User\",\n    {\"name\": doc.owner},\n    [\"full_name\"],\n    as_dict=True) %}\n{% set payment_schedules = set(frappe.db.get_all(\n    \"Payment Schedule\",\n    {\"parenttype\": \"Quotation\", \"parentfield\": \"payment_schedule\", \"parent\": doc.name},\n    pluck=\"payment_term\")) | reject(\"none\") | list %}\n    \n{% set contact = frappe.db.get_value(\n    \"Contact\",\n    {\"name\": doc.contact_person},\n    [\"full_name\", \"email_id\"],\n    as_dict=True) %}\n\n<div id=\"header-html\" class=\"hidden-pdf\">\n    <div class=\"row section-break\">\n        <div class=\"col-xs-2\">\n            <img class=\"logo\" src=\"{{ company.company_logo }}\" alt=\"Logo\">\n        </div>\n        <div class=\"col-xs-5\">\n            <strong class=\"title\">\n                {{ company.custom_company_name_en }}<br>\n                {{ company.company_name }}\n            </strong>\n        </div>\n        <div class=\"col-xs-2 zero-padding-left\">\n            <img class=\"iso-logo\" src=\"/assets/lpp_co/images/SGS-ISO-9001-UKAS_TCL_HR.jpg\" alt=\"Logo\">\n        </div>\n        <div class=\"col-xs-2 zero-padding-left\">\n            <img class=\"iso-logo\" src=\"/assets/lpp_co/images/SGS-ISO-14001-UKAS_TCL_HR.jpg\" alt=\"Logo\">\n        </div>\n        <div class=\"col-xs-1 page-number\">\n            {{ _(\"Page {0}/{1}\").format('<span class=\"page\"></span>', '<span class=\"topage\"></span>') }}\n        </div>\n    </div>\n    <div class=\"row section-break\">\n        <div class=\"col-xs-9\">\n            {{ company.custom_address_en or \"-\" }}<br>\n        </div>\n        <div class=\"col-xs-3 title title-quotation text-right\">\n            <strong>\n                \u0e43\u0e1a\u0e40\u0e2a\u0e19\u0e2d\u0e23\u0e32\u0e04\u0e32<br>\n                Quotation\n            </strong>\n        </div>\n    </div>\n    <div class=\"row section-break\">\n        <div class=\"col-xs-6\">\n            <div class=\"row section-break\">\n                <div class=\"col-xs-3\">\n                    <strong>\n                        \u0e25\u0e39\u0e01\u0e04\u0e49\u0e32<br>\n                        Customer\n                    </strong>\n                </div>\n                <div class=\"col-xs-9\">\n                    {{ doc.customer_name or \"-\" }}\n                </div>\n            </div>\n            <br>\n            <div class=\"row section-break\">\n                <div class=\"col-xs-3\">\n                    <strong>\n                        \u0e17\u0e35\u0e48\u0e2d\u0e22\u0e39\u0e48<br>\n                        Address\n                    </strong>\n                </div>\n                <div class=\"col-xs-9\">\n                    {% if customer_address %}\n                        {{ customer_address.address_line1 or \"\" }}\n                        {{ customer_address.address_line2 or \"\" }}\n                        {{ customer_address.city or \"\" }}\n                        {{ customer_address.county or \"\" }}\n                        {{ customer_address.state or \"\" }}\n                        {{ customer_address.pincode or \"\" }}\n                    {% else %}\n                        -\n                    {% endif %}\n                </div>\n            </div>\n            <br>\n            <div class=\"row section-break\">\n                <div class=\"col-xs-3\">\n                    <strong>\n                        \u0e15\u0e34\u0e14\u0e15\u0e48\u0e2d<br>\n                        Contact\n                    </strong>\n                </div>\n                <div class=\"col-xs-9\">\n                    {{ contact.full_name or \"-\" }}\n                    {% if contact.email_id %}\n                        | {{ contact.email_id }}\n                    {% endif %}\n                </div>\n            </div>\n        </div>\n        <div class=\"col-xs-6\">\n            <div class=\"row section-break\">\n                <div class=\"col-xs-7\">\n                    <strong>\u0e40\u0e25\u0e02\u0e17\u0e35\u0e48\u0e40\u0e2d\u0e01\u0e2a\u0e32\u0e23 / Document No.</strong>\n                </div>\n                <div class=\"col-xs-5\">\n                    {{ doc.name or \"-\" }}\n                </div>\n            </div>\n            <div class=\"row section-break\">\n                <div class=\"col-xs-7\">\n                    <strong>\u0e27\u0e31\u0e19\u0e17\u0e35\u0e48\u0e40\u0e2d\u0e01\u0e2a\u0e32\u0e23 / Document Date</strong>\n                </div>\n                <div class=\"col-xs-5\">\n                    {{ doc.transaction_date and frappe.utils.getdate(doc.transaction_date).strftime(\"%d/%m/%Y\") or \"-\" }}\n                </div>\n            </div>\n            <br>\n            <div class=\"row section-break\">\n                <div class=\"col-xs-7\">\n                    <strong>\u0e40\u0e2a\u0e19\u0e2d\u0e23\u0e32\u0e04\u0e32\u0e42\u0e14\u0e22 / Quoted By</strong>\n                </div>\n                <div class=\"col-xs-5\">\n                    {{ owner.full_name or \"-\" }}\n                </div>\n            </div>\n            <div class=\"row section-break\">\n                <div class=\"col-xs-7\">\n                    <strong>\n                        \u0e21\u0e35\u0e1c\u0e25\u0e08\u0e19\u0e16\u0e36\u0e07 / Valid Until\n                    </strong>\n                </div>\n                <div class=\"col-xs-5\">\n                    {{ doc.valid_till and frappe.utils.getdate(doc.valid_till).strftime(\"%d/%m/%Y\") or \"-\" }}\n                </div>\n            </div>\n            <div class=\"row section-break\">\n                <div class=\"col-xs-7\">\n                    <strong>\n                        \u0e40\u0e07\u0e37\u0e48\u0e2d\u0e19\u0e44\u0e02\u0e40\u0e04\u0e23\u0e14\u0e34\u0e15 / Credit Terms\n                    </strong>\n                </div>\n                <div class=\"col-xs-5\">\n                    {{ payment_schedules and payment_schedules | join(\", \") or \"-\" }}\n                </div>\n            </div>\n        </div>\n    </div>\n</div>\n<table>\n    <thead>\n        <tr>\n            <th class=\"table-head text-center col-1\" style=\"border-bottom-width: 2px !important;border-left: 1px solid #000000;\"><strong>\u0e25\u0e33\u0e14\u0e31\u0e1a<br>No.</strong></th>\n            <th class=\"table-head col-2\" style=\"border-bottom-width: 2px !important;\"><strong>\u0e23\u0e2b\u0e31\u0e2a\u0e2a\u0e34\u0e19\u0e04\u0e49\u0e32<br>Item Code</strong></th>\n            <th class=\"table-head col-3\" style=\"border-bottom-width: 2px !important;\"><strong>\u0e0a\u0e37\u0e48\u0e2d\u0e23\u0e32\u0e22\u0e01\u0e32\u0e23<br>Item Name</strong></th>\n            <th class=\"table-head text-right col-4\" style=\"border-bottom-width: 2px !important;\"><strong>\u0e08\u0e33\u0e19\u0e27\u0e19<br>Quantity</strong></th>\n            <th class=\"table-head text-right col-5\" style=\"border-bottom-width: 2px !important;\"><strong>\u0e2b\u0e19\u0e48\u0e27\u0e22<br>Unit</strong></th>\n            <th class=\"table-head text-right col-6\" style=\"border-bottom-width: 2px !important;\"><strong>\u0e23\u0e32\u0e04\u0e32<br>Price</strong></th>\n            <th class=\"table-head text-right col-7\" style=\"border-bottom-width: 2px !important;border-right: 1px solid #000000;\"><strong>\u0e08\u0e33\u0e19\u0e27\u0e19\u0e40\u0e07\u0e34\u0e19<br>Amount</strong></th>\n        </tr>\n    </thead>\n    <tbody>\n        {% for item in doc.items %}\n            <tr>\n                <td class=\"text-center\">\n                    {{ item.idx }}\n                </td>\n                <td>\n                    {{ item.item_code or \"-\" }}\n                </td>\n                <td>\n                    {{ item.description or \"-\" }}\n                </td>\n                <td class=\"text-right\">\n                    {{ \"{:,.2f}\".format(item.qty or 0) }}\n                </td>\n                <td class=\"text-right\">\n                    {{ item.uom or \"-\" }}\n                </td>\n                <td class=\"text-right\">\n                    {{ \"{:,.2f}\".format(item.rate) }}\n                </td>\n                <td class=\"text-right\">\n                    {{ \"{:,.2f}\".format(item.amount) }}\n                </td>\n            </tr>\n        {% endfor %}\n    </tbody>\n</table>\n<div id=\"footer-html\" class=\"visible-pdf\">\n    <p class=\"line-break\"></p>\n    <table class=\"page-break-inside\">\n        <tr>\n            <td class=\"col-foot-1\" rowspan=\"3\" style=\"padding-top: 0px !important;\">\n                <strong>Remark</strong><br>{{ doc.terms or \"-\" }}\n            </td>\n            <td class=\"col-foot-2\" style=\"padding-top: 0px !important;\">\n                <strong>\u0e22\u0e2d\u0e14\u0e23\u0e27\u0e21<br>Subtotal</strong>\n            </td>\n            <td class=\"col-foot-3\" style=\"padding-top: 0px !important;\">\n                {{ doc.currency }}\n            </td>\n            <td class=\"col-foot-4 text-right\" style=\"padding-top: 0px !important;\">\n                {{ \"{:,.2f}\".format(doc.total) }}\n            </td>\n        </tr>\n        <tr>\n            <td style=\"padding-top: 0px !important;\">\n                <strong>\u0e20\u0e32\u0e29\u0e35\u0e21\u0e39\u0e25\u0e04\u0e48\u0e32\u0e40\u0e1e\u0e34\u0e48\u0e21<br>VAT 7%</strong>\n            </td>\n            <td style=\"padding-top: 0px !important;\">\n                {{ doc.currency }}\n            </td>\n            <td class=\"text-right\" style=\"padding-top: 0px !important;\">\n                {{ \"{:,.2f}\".format(doc.total_taxes_and_charges) }}\n            </td>\n        </tr>\n        <tr>\n            <td style=\"padding-top: 0px !important;\">\n                <strong>\u0e22\u0e2d\u0e14\u0e23\u0e27\u0e21\u0e17\u0e31\u0e49\u0e07\u0e2a\u0e34\u0e49\u0e19<br>Grand Total</strong>\n            </td>\n            <td style=\"padding-top: 0px !important;\">\n                {{ doc.currency }}\n            </td>\n            <td class=\"text-right\" style=\"padding-top: 0px !important;\">\n                {{ \"{:,.2f}\".format(doc.grand_total) }}\n            </td>\n        </tr>\n    </table>\n    <div class=\"row page-break-inside\">\n        <div class=\"col-xs-12\">\n            <p class=\"color-thin\">\u0e17\u0e32\u0e07\u0e40\u0e23\u0e32\u0e2b\u0e27\u0e31\u0e07\u0e40\u0e1b\u0e47\u0e19\u0e2d\u0e22\u0e48\u0e32\u0e07\u0e22\u0e34\u0e48\u0e07\u0e27\u0e48\u0e32\u0e02\u0e49\u0e2d\u0e40\u0e2a\u0e19\u0e2d\u0e02\u0e2d\u0e07\u0e40\u0e23\u0e32\u0e08\u0e30\u0e44\u0e14\u0e49\u0e23\u0e31\u0e1a\u0e01\u0e32\u0e23\u0e1e\u0e34\u0e08\u0e32\u0e23\u0e13\u0e32\u0e40\u0e2b\u0e47\u0e19\u0e0a\u0e2d\u0e1a\u0e08\u0e32\u0e01\u0e17\u0e48\u0e32\u0e19 \u0e02\u0e2d\u0e1a\u0e04\u0e38\u0e13\u0e17\u0e35\u0e48\u0e43\u0e2b\u0e49\u0e04\u0e27\u0e32\u0e21\u0e44\u0e27\u0e49\u0e27\u0e32\u0e07\u0e43\u0e08\u0e43\u0e19\u0e1a\u0e23\u0e34\u0e01\u0e32\u0e23\u0e02\u0e2d\u0e07\u0e40\u0e23\u0e32</p>\n            <p class=\"color-thin\">We hope that our proposal will receive your favorable consideration. Thank you for trusting our services.</p>\n        </div>\n    </div>\n    <div class=\"row section-break\">\n        <div class=\"col-xs-4 text-center\">\n            <br><br><br>\n            <div style=\"border-top: 1px solid;\"></div>\n            <strong>\u0e1c\u0e39\u0e49\u0e23\u0e31\u0e1a\u0e40\u0e2d\u0e01\u0e2a\u0e32\u0e23 / Recipient</strong>\n        </div>\n        <div class=\"col-xs-4 text-center\">\n            <br><br><br>\n            <div style=\"border-top: 1px solid;\"></div>\n            <strong>\u0e1c\u0e39\u0e49\u0e40\u0e2a\u0e19\u0e2d\u0e23\u0e32\u0e04\u0e32 / Proposer</strong>\n        </div>\n        <div class=\"col-xs-4 text-center\">\n            <br><br><br>\n            <div style=\"border-top: 1px solid;\"></div>\n            <strong>\u0e1c\u0e39\u0e49\u0e2d\u0e19\u0e38\u0e21\u0e31\u0e15\u0e34 / Approver</strong>\n        </div>\n    </div>\n    <div class=\"row section-break\">\n        <div class=\"col-xs-4 text-center\">\n            <br><br>\n            <!--<div style=\"border-top: 1px dotted;\"></div>-->\n            <strong>\u0e27\u0e31\u0e19\u0e17\u0e35\u0e48 / Date _____________________________</strong>\n        </div>\n        <div class=\"col-xs-4 text-center\">\n            <br><br>\n            <strong>\u0e27\u0e31\u0e19\u0e17\u0e35\u0e48 / Date _____________________________</strong>\n        </div>\n        <div class=\"col-xs-4 text-center\">\n            <br><br>\n            <strong>\u0e27\u0e31\u0e19\u0e17\u0e35\u0e48 / Date _____________________________</strong>\n        </div>\n    </div>\n    <div class=\"row\">\n        <div class=\"col-xs-12 text-right\">\n            <strong>QR-MKT-003 Rev.N</strong> Affective from 01/01/2025\n        </div>\n    </div>\n</div>",
