 "doctype": "Print Format",
 "font_size": 14,
 "hide_if_not_default": 0,
 "html": "<!-- Load Fonts -->\n{{ frappe.render_template(\"templates/jinja_lpp_fonts.html\") }}\n\n<!-- Get value from doctype -->\n{% set company = frappe.db.get_value(\n    \"Company\",\n    {\"name\": doc.company},\n    [\"company_logo\", \"custom_company_name_th\", \"tax_id\", \"custom_company_name_en\"],\n    as_dict=True) %}\n{% set operations = set(frappe.db.get_all(\n    \"Work Order Operation\",\n    {\"parenttype\": \"Work Order\", \"parentfield\": \"operations\", \"parent\": doc.name},\n    pluck=\"workstation\")) | reject(\"none\") | list %}\n{% set customer_parts = set(frappe.db.get_all(\n    \"Item Customer Detail\",\n    {\"parenttype\": \"Item\", \"parentfield\": \"customer_items\", \"parent\": doc.production_item, \"customer_name\": doc.custom_customer},\n    pluck=\"ref_code\")) | reject(\"none\") | list %}\n{% set build_sheets = set(frappe.db.get_all(\n    \"Item Customer Detail\",\n    {\"parenttype\": \"Item\", \"parentfield\": \"customer_items\", \"parent\": doc.production_item, \"customer_name\": doc.custom_customer},\n    pluck=\"custom_drawing_build_sheet_no\")) | reject(\"none\") | list %}\n{% set batchs = set(frappe.db.get_all(\n    \"Batch\",\n    {\"reference_doctype\": \"Work Order\", \"reference_name\": doc.name},\n    pluck=\"name\")) | reject(\"none\") | list %}\n{% set batch_sizes = set(frappe.db.get_all(\n    \"BOM Operation\",\n    {\"parenttype\": \"BOM\", \"parentfield\": \"operations\", \"parent\": doc.bom_no},\n    pluck=\"batch_size\")) | reject(\"none\") | list %}\n{% set item = frappe.db.get_value(\n    \"Item\", \n    {\"name\": doc.production_item},\n    [\"custom_unit_pack\", \"custom_unit_box\", \"custom_mold_item\", \"custom_additional_process_\", \"custom_item_remark\"],\n    as_dict=True) %}\n    \n<div id=\"header-html\" class=\"hidden-pdf\">\n    <div class=\"row section-break\">\n        <div class=\"col-xs-2\">\n            <img class=\"logo\" src=\"{{ company.company_logo }}\" alt=\"Logo\">\n        </div>\n        <div class=\"col-xs-5\">\n            <strong class=\"title\">\n                {{ company.custom_company_name_en }}<br>\n                {{ company.custom_company_name_th }}\n            </strong>\n        </div>\n        <div class=\"col-xs-5 title title-work-order text-right\">\n            <strong>\n                \u0e43\u0e1a\u0e2a\u0e31\u0e48\u0e07\u0e07\u0e32\u0e19\u0e1c\u0e25\u0e34\u0e15<br>\n                Work Order\n            </strong>\n        </div>\n    </div>\n    <br>\n    <div class=\"row section-break\">\n        <div class=\"col-xs-6\">\n            <div class=\"row section-break\">\n                <div class=\"col-xs-4\">\n                    <strong>Customer</strong>\n                </div>\n                <div class=\"col-xs-8\">\n                    {{ doc.custom_customer_name or \"-\" }}\n                </div>\n            </div>\n            <div class=\"row section-break\">\n                <div class=\"col-xs-4\">\n                    <strong>Item Code</strong>\n                </div>\n                <div class=\"col-xs-8 font-red\">\n                    {{ doc.production_item or \"-\" }}\n                </div>\n            </div>\n            <div class=\"row section-break\">\n                <div class=\"col-xs-4\">\n                    <strong>Item Name</strong>\n                </div>\n                <div class=\"col-xs-8 font-red\">\n                    {{ doc.item_name or \"-\" }}\n                </div>\n            </div>\n            <div class=\"row section-break\">\n                <div class=\"col-xs-4\" style=\"padding-right: 0px;\">\n                    <strong>Customer Part No.</strong>\n                </div>\n                <div class=\"col-xs-8\">\n                    {{ customer_parts and customer_parts | join(\", \") or \"-\" }}\n                </div>\n            </div>\n            <div class=\"row section-break\">\n                <div class=\"col-xs-4\">\n                    <strong>Build Sheet No.</strong>\n                </div>\n                <div class=\"col-xs-8\">\n                    {{ build_sheets and build_sheets | join(\", \") or \"-\" }}\n                </div>\n            </div>\n            <div class=\"row section-break\">\n                <div class=\"col-xs-4\">\n                    <strong>Units / Pack</strong>\n                </div>\n                <div class=\"col-xs-8\">\n                    {{ item.custom_unit_pack or 0 }}\n                </div>\n            </div>\n            <div class=\"row section-break\">\n                <div class=\"col-xs-4\">\n                    <strong>Units / Box</strong>\n                </div>\n                <div class=\"col-xs-8\">\n                    {{ item.custom_unit_box or 0 }}\n                </div>\n            </div>\n            <div class=\"row section-break\">\n                <div class=\"col-xs-4\">\n                    <strong>Weight / Unit</strong>\n                </div>\n                <div class=\"col-xs-8\">\n                    {% set total_qty_per_unit = doc.required_items | sum(attribute=\"custom_qty_per_unit\") %}\n                    {{ \"{:,.0f}\".format(total_qty_per_unit) if total_qty_per_unit - total_qty_per_unit | int == 0 else  \"{:,.2f}\".format(total_qty_per_unit) }}\n                </div>\n            </div>\n            <div class=\"row section-break\">\n                <div class=\"col-xs-4\">\n                    <strong>Add. Process</strong>\n                </div>\n                <div class=\"col-xs-8\">\n                    {{ item.custom_additional_process_ or \"-\" }}\n                </div>\n            </div>\n            <div class=\"row section-break\">\n                <div class=\"col-xs-4\">\n                    <strong>Item Remark</strong>\n                </div>\n                <div class=\"col-xs-8\">\n                    {{ item.custom_item_remark or \"-\" }}\n                </div>\n            </div>\n        </div>\n        <div class=\"col-xs-6\">\n            <div class=\"row section-break\">\n                <div class=\"col-xs-7\">\n                    <span class=\"color-thin\">\u0e40\u0e25\u0e02\u0e17\u0e35\u0e48\u0e40\u0e2d\u0e01\u0e2a\u0e32\u0e23 / Document No.</span>\n                </div>\n                <div class=\"col-xs-5\">\n                    {{ doc.name or \"-\" }}\n                </div>\n            </div>\n            <div class=\"row section-break\">\n                <div class=\"col-xs-7\">\n                    <span class=\"color-thin\">\u0e27\u0e31\u0e19\u0e17\u0e35\u0e48\u0e40\u0e2d\u0e01\u0e2a\u0e32\u0e23 / Document Date</span>\n                </div>\n                <div class=\"col-xs-5\">\n                    {{ doc.creation and frappe.utils.getdate(doc.creation).strftime(\"%d/%m/%Y\") or \"-\" }}\n                </div>\n            </div>\n            <div class=\"row section-break\">\n                <div class=\"col-xs-7\">\n                    <strong>Batch No.</strong>\n                </div>\n                <div class=\"col-xs-5\">\n                    {{ batchs and batchs | join(\", \") or \"-\" }}\n                </div>\n            </div>\n            <div class=\"row section-break\">\n                <div class=\"col-xs-7\">\n                    <strong>Ordered Quantity</strong>\n                </div>\n                <div class=\"col-xs-5 font-red\">\n                    {{ \"{:,.0f}\".format(doc.qty) if doc.qty - doc.qty | int == 0 else  \"{:,.2f}\".format(doc.qty) }}\n                </div>\n            </div>\n            <div class=\"row section-break\">\n                <div class=\"col-xs-7\">\n                    <strong>Quantity / Job Card</strong>\n                </div>\n                <div class=\"col-xs-5\">\n                    {{ batch_sizes and batch_sizes | join(\", \") or \"-\" }}\n                </div>\n            </div>\n            <div class=\"row section-break\">\n                <div class=\"col-xs-7\">\n                    <strong>Total Job Cards</strong>\n                </div>\n                <div class=\"col-xs-5\">\n                    {% set total_job_cards = frappe.db.count(\"Job Card\", {\"work_order\": doc.name}) %}\n                    {{ total_job_cards }}\n                </div>\n            </div>\n            <div class=\"row section-break\">\n                <div class=\"col-xs-7\">\n                    <strong>Workstation</strong>\n                </div>\n                <div class=\"col-xs-5 font-red\">\n                    {{ operations and operations | join(\", \") or \"-\" }}\n                </div>\n            </div>\n            <div class=\"row section-break\">\n                <div class=\"col-xs-7\">\n                    <strong>Mold</strong>\n                </div>\n                <div class=\"col-xs-5\">\n                    {% if item.custom_mold_item %}\n                        {% set mold_item = frappe.db.get_value(\n                            \"Item\", \n                            {\"name\": item.custom_mold_item},\n                            [\"item_name\"],\n                            as_dict=True) %}\n                        {{ mold_item.item_name or \"-\" }}\n                    {% else %}\n                        -\n                    {% endif %}\n                </div>\n            </div>\n            <div class=\"row section-break\">\n                <div class=\"col-xs-7\">\n                    <strong>BOM ID</strong>\n                </div>\n                <div class=\"col-xs-5\">\n                    {{ doc.bom_no or \"-\" }}\n                </div>\n            </div>\n            <div class=\"row section-break\">\n                <div class=\"col-xs-7\">\n                    <strong>Remark</strong>\n                </div>\n                <div class=\"col-xs-5\">\n                    {{ doc.custom_remarks or \"-\" }}\n                </div>\n            </div>\n        </div>\n    </div>\n</div>\n<table>\n    <thead>\n        <th class=\"table-head text-center col-1\" style=\"border-left: 1px solid #6D6E71;\"><strong>\u0e25\u0e33\u0e14\u0e31\u0e1a<br>No.</strong></th>\n        <th class=\"table-head col-2\"><strong>\u0e23\u0e2b\u0e31\u0e2a\u0e2a\u0e34\u0e19\u0e04\u0e49\u0e32<br>Item Code</strong></th>\n        <th class=\"table-head col-3\"><strong>\u0e0a\u0e37\u0e48\u0e2d\u0e23\u0e32\u0e22\u0e01\u0e32\u0e23<br>Item Name</strong></th>\n        <th class=\"table-head text-right col-4\"><strong>Percent</strong></th>\n        <th class=\"table-head text-right col-5\"><strong>Qty / Unit</strong></th>\n        <th class=\"table-head text-right col-6\"><strong>\u0e22\u0e2d\u0e14\u0e04\u0e07\u0e04\u0e25\u0e31\u0e07<br>Inventory</strong></th>\n        <th class=\"table-head text-right col-7\"><strong>\u0e15\u0e49\u0e2d\u0e07\u0e43\u0e0a\u0e49<br>Required</strong></th>\n        <th class=\"table-head text-right col-8\" style=\"border-right: 1px solid #6D6E71;\"><strong>\u0e2b\u0e19\u0e48\u0e27\u0e22<br>Unit</strong></th>\n    </thead>\n    <tbody>\n        {% for item in doc.required_items %}\n            <tr>\n                <td class=\"text-center\">\n                    {{ item.idx }}\n                </td>\n                <td>\n                    {{ item.item_code or \"-\" }}\n                </td>\n                <td>\n                    {{ item.description or \"-\" }}\n                </td>\n                <td class=\"text-right\">\n                    {{ \"{:,.2f}\".format(item.custom_percent_per_unit) }} %\n                </td>\n                <td class=\"text-right\">\n                    {{ \"{:,.2f}\".format(item.custom_qty_per_unit) }}\n                </td>\n                <td class=\"text-right\">\n                    {{ \"{:,.2f}\".format(item.available_qty_at_source_warehouse) }}\n                </td>\n                <td class=\"text-right\">\n                    {{ \"{:,.2f}\".format(item.required_qty) }}\n                </td>\n                <td class=\"text-right\">\n                    {{ item.stock_uom or \"-\" }}\n                </td>\n            </tr>\n        {% endfor %}\n    </tbody>\n</table>\n<br>\n<p class=\"line-break\"></p>\n<div class=\"page-break-inside\">\n    <div class=\"row section-break\">\n        <div class=\"col-xs-12\">\n            <strong>Operations</strong>\n        </div>\n    </div>\n    {% for operation in doc.operations %}\n        <div class=\"row section-break\">\n            <div class=\"col-xs-1\">\n                {{ operation.idx }}\n            </div>\n            <div class=\"col-xs-6\">\n                {{ operation.operation or \"-\" }}\n            </div>\n            <div class=\"col-xs-5\">\n                \u0e23\u0e31\u0e1a\u0e1c\u0e34\u0e14\u0e0a\u0e2d\u0e1a\u0e42\u0e14\u0e22 <div class=\"underline\"></div>\n            </div>\n        </div>\n    {% endfor %}\n</div>\n{% set number_of_tables = 3 %}\n{% set column_width = \"12%\" %}\n{% for i in range(number_of_tables) %}\n    <br>\n    <table class=\"page-break-inside\">\n        <tbody>\n            <tr>\n                <td class=\"add-border\" style=\"width: 16%;\">\u0e27\u0e31\u0e19\u0e17\u0e35\u0e48</td>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\"></td>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\"></td>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\"></td>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\"></td>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\"></td>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\"></td>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\"></td>\n            </tr>\n            <tr>\n                <td class=\"add-border\" style=\"width: 16%;\">\u0e01\u0e30</td>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\"></td>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\"></td>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\"></td>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\"></td>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\"></td>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\"></td>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\"></td>\n            </tr>\n            <tr>\n                <td class=\"add-border\" style=\"width: 16%;\">\u0e08\u0e33\u0e19\u0e27\u0e19\u0e01\u0e30</td>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\"></td>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\"></td>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\"></td>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\"></td>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\"></td>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\"></td>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\"></td>\n            </tr>\n            <tr>\n                <td class=\"add-border\" style=\"width: 16%;\">\u0e08\u0e33\u0e19\u0e27\u0e19\u0e23\u0e27\u0e21\u0e2a\u0e30\u0e2a\u0e21</td>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\"></td>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\"></td>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\"></td>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\"></td>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\"></td>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\"></td>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\"></td>\n            </tr>\n        </tbody>\n    </table>\n{% endfor %}\n<div id=\"footer-html\" class=\"visible-pdf\">\n    <div class=\"row\">\n        <div class=\"col-xs-12 text-right\">\n            <strong>QR-PC-001 Rev.B</strong> <span class=\"color-thin\">- Affective from 01/01/2025</span>\n        </div>\n    </div>\n</div>",
