 "css": "body {\n    padding: 3mm 0 3mm !important;\n}\n.print-format {\n    line-height: 1.5;\n    margin-left: 5mm;\n    margin-right: 5mm;\n    font-family: \"NotoSansThaiLooped\";\n}\n.print-format, .print-format td, .print-format th {\n    font-size: 12px !important;\n    color: #000000 !important;\n}\n.print-format-preview{\n    padding: 5mm;\n}\n.logo {\n    margin-top: 10px;\n    width: 100px;\n}\n.title {\n    font-size: 18px;\n    font-family: \"NotoSansThai\";\n}\n.title-work-order {\n    font-size: 28px !important;\n}\ntable {\n    width: 100%;\n}\n.table-head {\n    border-top: 1px solid #6D6E71;\n    border-bottom: 1px solid #6D6E71;\n}\n.line-break {\n    border-top: 1px solid #6D6E71;\n}\n.col-1 {\n    width: 5%;\n}\n.col-3 {\n    width: 21%;\n}\n.col-4 {\n    width: 10%;\n}\n.col-5 {\n    width: 12%;\n}\n.col-2, .col-6, .col-7, .col-8 {\n    width: 13%;\n}\n.col-foot-1 {\n    width: 56%;\n}\n.col-foot-2 {\n    width: 22%;\n}\n.col-foot-3 {\n    width: 22%;\n}\n.page-break-inside {\n    page-break-inside: avoid;\n}\n.color-thin{\n    color: #6D6E71 !important;\n}\n.underline {\n    width: 200px;\n    border-bottom: 1px solid #000000;\n    display: inline-block;\n}\n.add-border {\n    border: 1px solid #000000;\n}\n.font-red {\n    color: red;\n}",
 "custom_format": 1,
 "default_condition": "True",
 "default_print_language": "en",
