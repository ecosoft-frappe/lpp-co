 "css": "body {\n    padding: 0mm 0 5mm !important;\n}\n.print-format {\n    line-height: 1.5;\n    margin-left: 5mm;\n    margin-right: 5mm;\n}\n.print-format, .print-format td, .print-format th {\n    font-size: 12px !important;\n    color: #000000 !important;\n}\n.print-format-preview{\n    padding: 5mm;\n}\n.logo {\n    margin-top: 10px;\n    width: 100px;\n}\n.title {\n    font-size: 18px;\n}\ntable {\n    width: 100%;\n}\n.table-head {\n    background-color: #FF8080;\n    border: 1px solid #000000;\n}\n.table-detail {\n    border-bottom: 1px solid #d3d3d3;\n}\n.col-1 {\n    width: 5%;\n}\n.col-2 {\n    width: 16%;\n}\n.col-3 {\n    width: 35%;\n}\n.col-4, .col-5, .col-6, .col-7 {\n    width: 11%;\n}\n.col-foot-1 {\n    width: 56%;\n}\n.col-foot-2 {\n    width: 22%;\n}\n.col-foot-3 {\n    width: 22%;\n}\n.page-break-inside {\n    page-break-inside: avoid;\n}\n.color-thin{\n    color: #6D6E71 !important;\n}\n.underline {\n    width: 200px;\n    border-bottom: 1px solid #000000;\n    display: inline-block;\n}\n.add-border {\n    border: 1px solid #000000;\n}",
 "custom_format": 1,
 "default_print_language": "en",
 "disabled": 0,
 "doc_type": "Work Order",
 "docstatus": 0,
 "doctype": "Print Format",
 "font_size": 14,
 "hide_if_not_default": 0,
 "html": "<!-- Get value from doctype -->\n{% set company = frappe.db.get_value(\n    \"Company\",\n    {\"name\": doc.company},\n    [\"company_logo\", \"company_name\", \"tax_id\"],\n    as_dict=True) %}\n{% set parent_company_address = frappe.db.get_all(\n    \"Dynamic Link\",\n    {\"link_doctype\": \"Company\", \"link_name\": company.company_name},\n    pluck=\"parent\") %}\n{% set company_address = frappe.db.get_value(\n    \"Address\",\n    {\"name\": (\"in\", parent_company_address), \"address_type\": \"Permanent\"},\n    [\"address_line1\", \"address_line2\", \"city\", \"county\", \"state\", \"pincode\", \"phone\", \"fax\", \"email_id\"],\n    as_dict=True) %}\n{% set operations = set(frappe.db.get_all(\n    \"Work Order Operation\",\n    {\"parenttype\": \"Work Order\", \"parentfield\": \"operations\", \"parent\": doc.name},\n    pluck=\"workstation\")) | reject(\"none\") | list %}\n{% set materials = set(frappe.db.get_all(\n    \"Item Customer Detail\",\n    {\"parenttype\": \"Item\", \"parentfield\": \"customer_items\", \"parent\": doc.production_item},\n    pluck=\"custom_material\")) | reject(\"none\") | list %}\n{% set build_sheets = set(frappe.db.get_all(\n    \"Item Customer Detail\",\n    {\"parenttype\": \"Item\", \"parentfield\": \"customer_items\", \"parent\": doc.production_item},\n    pluck=\"custom_drawing_build_sheet_no\")) | reject(\"none\") | list %}\n{% set batchs = set(frappe.db.get_all(\n    \"Batch\",\n    {\"reference_doctype\": \"Work Order\", \"reference_name\": doc.name},\n    pluck=\"name\")) | reject(\"none\") | list %}\n{% set batch_sizes = set(frappe.db.get_all(\n    \"BOM Operation\",\n    {\"parenttype\": \"BOM\", \"parentfield\": \"operations\", \"parent\": doc.bom_no},\n    pluck=\"batch_size\")) | reject(\"none\") | list %}\n    \n<div id=\"header-html\" class=\"hidden-pdf\">\n    <div class=\"row section-break\">\n        <div class=\"col-xs-2\">\n            <img class=\"logo\" src=\"{{ company.company_logo }}\" alt=\"Logo\">\n        </div>\n        <div class=\"col-xs-10\">\n            <strong class=\"title\">{{ company.company_name }}</strong>\n            <p>\n                {% if company_address %}\n                    {{ company_address.address_line1 or \"\" }}\n                    {{ company_address.address_line2 or \"\" }}\n                    {{ company_address.city or \"\" }}\n                    {{ company_address.county or \"\" }}\n                    {{ company_address.state or \"\" }}\n                    {{ company_address.pincode or \"\" }}\n                {% else %}\n                    -\n                {% endif %}\n            </p>\n            <p>\n                TEL: {{ company_address.phone or \"-\" }} FAX: {{ company_address.fax or \"-\" }} Email: {{ company_address.email_id or \"-\" }}\n            </p>\n            <p>\u0e40\u0e25\u0e02\u0e1b\u0e23\u0e30\u0e08\u0e33\u0e15\u0e31\u0e27\u0e1c\u0e39\u0e49\u0e40\u0e2a\u0e35\u0e22\u0e20\u0e32\u0e29\u0e35 / Tax ID {{ company.tax_id or \"-\" }} \u0e2a\u0e33\u0e19\u0e31\u0e01\u0e07\u0e32\u0e19\u0e43\u0e2b\u0e0d\u0e48</p>\n        </div>\n    </div>\n    <div class=\"row section-break\">\n        <div class=\"col-xs-9\"></div>\n        <div class=\"col-xs-3 title text-center\">\n            <strong>\u0e43\u0e1a\u0e2a\u0e31\u0e48\u0e07\u0e07\u0e32\u0e19\u0e1c\u0e25\u0e34\u0e15<br>Work Order</strong>\n        </div>\n    </div>\n    <br>\n    <div class=\"row section-break\">\n        <div class=\"col-xs-6\">\n            <div class=\"row section-break\">\n                <div class=\"col-xs-4\">\n                    <strong>Customer:</strong>\n                </div>\n                <div class=\"col-xs-8\">\n                    {{ doc.custom_customer_name or \"-\" }}\n                </div>\n            </div>\n            <div class=\"row section-break\">\n                <div class=\"col-xs-4\">\n                    <strong>Item Code:</strong>\n                </div>\n                <div class=\"col-xs-8\">\n                    {{ doc.production_item or \"-\" }}\n                </div>\n            </div>\n            <div class=\"row section-break\">\n                <div class=\"col-xs-4\">\n                    <strong>Item Name:</strong>\n                </div>\n                <div class=\"col-xs-8\">\n                    {{ doc.item_name or \"-\" }}\n                </div>\n            </div>\n            <div class=\"row section-break\">\n                <div class=\"col-xs-4\">\n                    <strong>Material:</strong>\n                </div>\n                <div class=\"col-xs-8\">\n                    {{ materials and materials | join(\", \") or \"-\" }}\n                </div>\n            </div>\n            <div class=\"row section-break\">\n                <div class=\"col-xs-4\">\n                    <strong>Build Sheet No:</strong>\n                </div>\n                <div class=\"col-xs-8\">\n                    {{ build_sheets and build_sheets | join(\", \") or \"-\" }}\n                </div>\n            </div>\n            <div class=\"row section-break\">\n                <div class=\"col-xs-4\">\n                    <strong>Units / Pack:</strong>\n                </div>\n                <div class=\"col-xs-8\">\n                    (\u0e44\u0e21\u0e48\u0e21\u0e35\u0e02\u0e49\u0e2d\u0e21\u0e39\u0e25)\n                </div>\n            </div>\n            <div class=\"row section-break\">\n                <div class=\"col-xs-4\">\n                    <strong>Units / Box:</strong>\n                </div>\n                <div class=\"col-xs-8\">\n                    (\u0e44\u0e21\u0e48\u0e21\u0e35\u0e02\u0e49\u0e2d\u0e21\u0e39\u0e25)\n                </div>\n            </div>\n            <div class=\"row section-break\">\n                <div class=\"col-xs-4\">\n                    <strong>Weight / Unit:</strong>\n                </div>\n                <div class=\"col-xs-8\">\n                    (\u0e44\u0e21\u0e48\u0e21\u0e35\u0e02\u0e49\u0e2d\u0e21\u0e39\u0e25)\n                </div>\n            </div>\n            <div class=\"row section-break\">\n                <div class=\"col-xs-4\">\n                    <strong>Add. Process:</strong>\n                </div>\n                <div class=\"col-xs-8\">\n                    (\u0e44\u0e21\u0e48\u0e21\u0e35\u0e02\u0e49\u0e2d\u0e21\u0e39\u0e25)\n                </div>\n            </div>\n            <div class=\"row section-break\">\n                <div class=\"col-xs-4\">\n                    <strong>Item Remark:</strong>\n                </div>\n                <div class=\"col-xs-8\">\n                    (\u0e44\u0e21\u0e48\u0e21\u0e35\u0e02\u0e49\u0e2d\u0e21\u0e39\u0e25)\n                </div>\n            </div>\n        </div>\n        <div class=\"col-xs-6\">\n            <div class=\"row section-break\">\n                <div class=\"col-xs-5\">\n                    <strong>Document No:</strong>\n                </div>\n                <div class=\"col-xs-7\">\n                    {{ doc.name or \"-\" }}\n                </div>\n            </div>\n            <div class=\"row section-break\">\n                <div class=\"col-xs-5\">\n                    <strong>Document Date:</strong>\n                </div>\n                <div class=\"col-xs-7\">\n                    {{ doc.creation and frappe.utils.getdate(doc.creation).strftime(\"%d/%m/%Y\") or \"-\" }}\n                </div>\n            </div>\n            <div class=\"row section-break\">\n                <div class=\"col-xs-5\">\n                    <strong>Batch No:</strong>\n                </div>\n                <div class=\"col-xs-7\">\n                    {{ batchs and batchs | join(\", \") or \"-\" }}\n                </div>\n            </div>\n            <div class=\"row section-break\">\n                <div class=\"col-xs-5\">\n                    <strong>Ordered Quantity:</strong>\n                </div>\n                <div class=\"col-xs-7\">\n                    {{ \"{:,.0f}\".format(doc.qty) if doc.qty - doc.qty | int == 0 else  \"{:,.2f}\".format(doc.qty) }}\n                </div>\n            </div>\n            <div class=\"row section-break\">\n                <div class=\"col-xs-5\">\n                    <strong>Quantity / Job Card:</strong>\n                </div>\n                <div class=\"col-xs-7\">\n                    {{ batch_sizes and batch_sizes | join(\", \") or \"-\" }}\n                </div>\n            </div>\n            <div class=\"row section-break\">\n                <div class=\"col-xs-5\">\n                    <strong>Total Job Cards:</strong>\n                </div>\n                <div class=\"col-xs-7\">\n                    {% set total_job_cards = frappe.db.count(\"Job Card\", {\"work_order\": doc.name}) %}\n                    {{ total_job_cards }}\n                </div>\n            </div>\n            <div class=\"row section-break\">\n                <div class=\"col-xs-5\">\n                    <strong>Workstation:</strong>\n                </div>\n                <div class=\"col-xs-7\">\n                    {{ operations and operations | join(\", \") or \"-\" }}\n                </div>\n            </div>\n            <div class=\"row section-break\">\n                <div class=\"col-xs-5\">\n                    <strong>Mold:</strong>\n                </div>\n                <div class=\"col-xs-7\">\n                    (\u0e44\u0e21\u0e48\u0e21\u0e35\u0e02\u0e49\u0e2d\u0e21\u0e39\u0e25)\n                </div>\n            </div>\n            <div class=\"row section-break\">\n                <div class=\"col-xs-5\">\n                    <strong>BOM ID:</strong>\n                </div>\n                <div class=\"col-xs-7\">\n                    {{ doc.bom_no or \"-\" }}\n                </div>\n            </div>\n            <div class=\"row section-break\">\n                <div class=\"col-xs-5\">\n                    <strong>BOM Remark:</strong>\n                </div>\n                <div class=\"col-xs-7\">\n                    (\u0e44\u0e21\u0e48\u0e21\u0e35\u0e02\u0e49\u0e2d\u0e21\u0e39\u0e25)\n                </div>\n            </div>\n        </div>\n    </div>\n</div>\n<table>\n    <thead>\n        <th class=\"table-head text-center col-1\" style=\"border-bottom-width: 2px !important;\"><strong>\u0e25\u0e33\u0e14\u0e31\u0e1a</strong><br>No</th>\n        <th class=\"table-head text-center col-2\" style=\"border-bottom-width: 2px !important;\"><strong>\u0e23\u0e2b\u0e31\u0e2a\u0e2a\u0e34\u0e19\u0e04\u0e49\u0e32</strong><br>Item Code</th>\n        <th class=\"table-head text-center col-3\" style=\"border-bottom-width: 2px !important;\"><strong>\u0e0a\u0e37\u0e48\u0e2d\u0e23\u0e32\u0e22\u0e01\u0e32\u0e23</strong><br>Item Name</th>\n        <th class=\"table-head text-center col-4\" style=\"border-bottom-width: 2px !important;\"><strong>\u0e2a\u0e31\u0e14\u0e2a\u0e48\u0e27\u0e19</strong><br>Proportion</th>\n        <th class=\"table-head text-center col-5\" style=\"border-bottom-width: 2px !important;\"><strong>\u0e22\u0e2d\u0e14\u0e04\u0e07\u0e04\u0e25\u0e31\u0e07</strong><br>Inventory</th>\n        <th class=\"table-head text-center col-6\" style=\"border-bottom-width: 2px !important;\"><strong>\u0e15\u0e49\u0e2d\u0e07\u0e43\u0e0a\u0e49</strong><br>Required</th>\n        <th class=\"table-head text-center col-7\" style=\"border-bottom-width: 2px !important;\"><strong>\u0e2b\u0e19\u0e48\u0e27\u0e22</strong><br>Unit</th>\n    </thead>\n    <tbody>\n        {% for item in doc.required_items %}\n            <tr>\n                <td class=\"table-detail text-center\">\n                    {{ item.idx }}\n                </td>\n                <td class=\"table-detail\">\n                    {{ item.item_code or \"-\" }}\n                </td>\n                <td class=\"table-detail\">\n                    {{ item.description or \"-\" }}\n                </td>\n                <td class=\"table-detail\">\n                    (\u0e44\u0e21\u0e48\u0e21\u0e35\u0e02\u0e49\u0e2d\u0e21\u0e39\u0e25)\n                </td>\n                <td class=\"table-detail text-right\">\n                    {{ \"{:,.2f}\".format(item.available_qty_at_source_warehouse) }}\n                </td>\n                <td class=\"table-detail text-right\">\n                    {{ \"{:,.2f}\".format(item.required_qty) }}\n                </td>\n                <td class=\"table-detail\">\n                    {{ item.stock_uom or \"-\" }}\n                </td>\n            </tr>\n        {% endfor %}\n    </tbody>\n</table>\n<br>\n<div class=\"page-break-inside\">\n    <div class=\"row section-break\">\n        <div class=\"col-xs-12\">\n            \u0e02\u0e31\u0e49\u0e19\u0e15\u0e2d\u0e19\u0e01\u0e32\u0e23\u0e1c\u0e25\u0e34\u0e15\n        </div>\n    </div>\n    {% for operation in doc.operations %}\n        <div class=\"row section-break\">\n            <div class=\"col-xs-1\">\n                {{ operation.idx }}\n            </div>\n            <div class=\"col-xs-6\">\n                {{ operation.operation or \"-\" }}\n            </div>\n            <div class=\"col-xs-5\">\n                \u0e23\u0e31\u0e1a\u0e1c\u0e34\u0e14\u0e0a\u0e2d\u0e1a\u0e42\u0e14\u0e22 <div class=\"underline\"></div>\n            </div>\n        </div>\n    {% endfor %}\n</div>\n{% set number_of_tables = 3 %}\n{% set column_width = \"20%\" %}\n{% for i in range(number_of_tables) %}\n    <br>\n    <table class=\"page-break-inside\">\n        <tbody>\n            <tr>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\">\u0e27\u0e31\u0e19\u0e17\u0e35\u0e48</td>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\"></td>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\"></td>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\"></td>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\"></td>\n            </tr>\n            <tr>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\">\u0e01\u0e30</td>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\"></td>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\"></td>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\"></td>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\"></td>\n            </tr>\n            <tr>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\">\u0e08\u0e33\u0e19\u0e27\u0e19\u0e01\u0e30</td>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\"></td>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\"></td>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\"></td>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\"></td>\n            </tr>\n            <tr>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\">\u0e08\u0e33\u0e19\u0e27\u0e19\u0e23\u0e27\u0e21\u0e2a\u0e30\u0e2a\u0e21</td>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\"></td>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\"></td>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\"></td>\n                <td class=\"add-border\" style=\"width: {{ column_width }};\"></td>\n            </tr>\n        </tbody>\n    </table>\n{% endfor %}",
