 "css": "th {\n    font-size: 12px;\n}\n.print-format {\n    padding: 0px;\n    margin-right: 5mm;\n    margin-left: 5mm;\n    margin-top: 3mm;\n    margin-bottom: 0mm;\n}\n.inline-header {\n    display: inline-block;\n    margin: 0 10px;\n    border-top: 2px solid #000000;\n    width: 90%;\n    text-align: center;\n    line-height: 30px;\n    vertical-align: top;\n}",
