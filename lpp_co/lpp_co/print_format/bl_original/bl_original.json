 "html": "<!-- Load Fonts -->\n{{ frappe.render_template(\"templates/jinja_lpp_fonts.html\") }}\n\n<!-- Get value from doctype -->\n{% set company = frappe.db.get_value(\n    \"Company\",\n    {\"name\": doc.company},\n    [\"company_logo\", \"custom_company_name_th\", \"custom_company_name_en\", \"custom_address_en\"],\n    as_dict=True) %}\n{% set customer = frappe.db.get_value(\n    \"Customer\",\n    {\"name\": doc.customer},\n    [\"tax_id\", \"branch_code\"],\n    as_dict=True) %}\n\n<div id=\"header-html\" class=\"hidden-pdf\">\n    <div class=\"row section-break\">\n        <div class=\"col-xs-2\">\n            <img class=\"logo\" src=\"{{ company.company_logo }}\" alt=\"Logo\">\n        </div>\n        <div class=\"col-xs-5\">\n            <strong class=\"title\">\n                {{ company.custom_company_name_en }}<br>\n                {{ company.custom_company_name_th }}\n            </strong>\n        </div>\n        <div class=\"col-xs-5 text-right\">\n            <span class=\"color-thin\">Page</span> {{ _(\"{0}/{1}\").format('<span class=\"page\"></span>', '<span class=\"topage\"></span>') }}\n        </div>\n    </div>\n    <div class=\"row section-break\">\n        <div class=\"col-xs-8\">\n            {{ company.custom_address_en or \"-\" }}<br>\n        </div>\n        <div class=\"col-xs-4 title title-sales-billing text-right\">\n            <strong>\n                \u0e43\u0e1a\u0e27\u0e32\u0e07\u0e1a\u0e34\u0e25<br>\n                Sales Billing\n            </strong>\n        </div>\n    </div>\n    <div class=\"row section-break\">\n        <div class=\"col-xs-7\">\n            <div class=\"row section-break\">\n                <div class=\"col-xs-2\">\n                    <strong>\n                        \u0e25\u0e39\u0e01\u0e04\u0e49\u0e32<br>\n                        Customer\n                    </strong>\n                </div>\n                <div class=\"col-xs-10\">\n                    {{ doc.customer_name or \"-\" }}\n                </div>\n            </div>\n            <br>\n            <div class=\"row section-break\">\n                <div class=\"col-xs-2\">\n                    <strong>\n                        \u0e17\u0e35\u0e48\u0e2d\u0e22\u0e39\u0e48<br>\n                        Address\n                    </strong>\n                </div>\n                <div class=\"col-xs-10\">\n                    {% if doc.sales_billing_line %}\n                        {% set sales_invoice = doc.sales_billing_line[0].sales_invoice %}\n                        {% if sales_invoice %}\n                            {% set si = frappe.get_doc(\"Sales Invoice\", sales_invoice) %}\n                            {% set customer_address = frappe.db.get_value(\n                                \"Address\",\n                                {\"name\": (\"=\", si.customer_address)},\n                                [\"address_line1\", \"address_line2\", \"city\", \"county\", \"state\", \"pincode\"],\n                                as_dict=True) %}\n                            {% if customer_address %}\n                                {{ customer_address.address_line1 or \"\" }}\n                                {{ customer_address.address_line2 or \"\" }}\n                                {{ customer_address.city or \"\" }}\n                                {{ customer_address.county or \"\" }}\n                                {{ customer_address.state or \"\" }}\n                                {{ customer_address.pincode or \"\" }}\n                            {% else %}\n                                -\n                            {% endif %}\n                        {% else %}\n                            -\n                        {% endif %}\n                    {% else %}\n                        -\n                    {% endif %}\n                </div>\n            </div>\n            <br>\n            <div class=\"row section-break\">\n                <div class=\"col-xs-5\">\n                    <strong>\n                        \u0e40\u0e25\u0e02\u0e1b\u0e23\u0e30\u0e08\u0e33\u0e15\u0e31\u0e27\u0e1c\u0e39\u0e49\u0e40\u0e2a\u0e35\u0e22\u0e20\u0e32\u0e29\u0e35<br>\n                        Tax ID\n                    </strong>\n                </div>\n                <div class=\"col-xs-3\">\n                    {{ customer.tax_id or \"-\" }}\n                </div>\n                <div class=\"col-xs-2\">\n                    <strong>\n                        \u0e2a\u0e32\u0e02\u0e32<br>\n                        Branch\n                    </strong>\n                </div>\n                <div class=\"col-xs-2\">\n                    {% if customer.branch_code == \"00000\" %}\n                        Headquarters\n                    {% else %}\n                        {{ customer.branch_code or \"-\" }}\n                    {% endif %}\n                </div>\n            </div>\n        </div>\n        <div class=\"col-xs-5\">\n            <div class=\"row section-break\">\n                <div class=\"col-xs-8\">\n                    <span class=\"color-thin\">\n                        \u0e40\u0e25\u0e02\u0e17\u0e35\u0e48\u0e40\u0e2d\u0e01\u0e2a\u0e32\u0e23 / Document No.\n                    </span>\n                </div>\n                <div class=\"col-xs-4\">\n                    {{ doc.name or \"-\" }}\n                </div>\n            </div>\n            <div class=\"row section-break\">\n                <div class=\"col-xs-8\">\n                    <span class=\"color-thin\">\n                        \u0e27\u0e31\u0e19\u0e17\u0e35\u0e48\u0e40\u0e2d\u0e01\u0e2a\u0e32\u0e23 / Document Date\n                    </span>\n                </div>\n                <div class=\"col-xs-4\">\n                    {{ doc.date and frappe.utils.getdate(doc.date).strftime(\"%d/%m/%Y\") or \"-\" }}\n                </div>\n            </div>\n        </div>\n    </div>\n</div>\n<table>\n    <thead>\n        <th class=\"table-head text-center col-1\" style=\"border-left: 1px solid #6D6E71;\"><strong>\u0e25\u0e33\u0e14\u0e31\u0e1a<br>No.</strong></th>\n        <th class=\"table-head col-2\"><strong>\u0e40\u0e25\u0e02\u0e17\u0e35\u0e48\u0e43\u0e1a\u0e01\u0e33\u0e01\u0e31\u0e1a\u0e20\u0e32\u0e29\u0e35<br>Tax Invoice No.</strong></th>\n        <th class=\"table-head col-3\"><strong>\u0e27\u0e31\u0e19\u0e17\u0e35\u0e48\u0e43\u0e1a\u0e01\u0e33\u0e01\u0e31\u0e1a\u0e20\u0e32\u0e29\u0e35<br>Tax Invoice Date</strong></th>\n        <th class=\"table-head col-4\"><strong>\u0e2d\u0e49\u0e32\u0e07\u0e2d\u0e34\u0e07\u0e43\u0e1a\u0e2a\u0e31\u0e48\u0e07\u0e0b\u0e37\u0e49\u0e2d<br>Referenced PO No.</strong></th>\n        <th class=\"table-head text-right col-5\" style=\"border-right: 1px solid #6D6E71;\"><strong>\u0e08\u0e33\u0e19\u0e27\u0e19\u0e40\u0e07\u0e34\u0e19<br>Amount</strong></th>\n    </thead>\n    <tbody>\n        {% for item in doc.sales_billing_line %}\n            <tr>\n                <td class=\"text-center\">\n                    {{ item.idx }}\n                </td>\n                <td>\n                    {{ item.sales_invoice or \"-\" }}\n                </td>\n                <td>\n                    {{ item.invoice_date and frappe.utils.getdate(item.invoice_date).strftime(\"%d/%m/%Y\") or \"-\" }}\n                </td>\n                <td>\n                    {{ item.reference or \"-\" }}\n                </td>\n                <td class=\"text-right\">\n                    {{ \"{:,.2f}\".format(item.grand_total) }}\n                </td>\n            </tr>\n        {% endfor %}\n    </tbody>\n</table>\n<div id=\"footer-html\" class=\"visible-pdf\">\n    <p class=\"line-break\"></p>\n    <div class=\"row\">\n        <div class=\"col-xs-12\">\n            <span class=\"color-thin\">Remark</span><br>{{ doc.narration or \"-\" }}\n        </div>\n    </div>\n    <div class=\"row section-break\">\n        <div class=\"col-xs-1\"></div>\n        <div class=\"col-xs-4 text-center\">\n            <br><br><br>\n            <div style=\"border-top: 1px solid;\"></div>\n            <strong>\u0e1c\u0e39\u0e49\u0e23\u0e31\u0e1a\u0e27\u0e32\u0e07\u0e1a\u0e34\u0e25 / Receiving Person</strong>\n        </div>\n        <div class=\"col-xs-2\"></div>\n        <div class=\"col-xs-4 text-center\">\n            <br><br><br>\n            <div style=\"border-top: 1px solid;\"></div>\n            <strong>\u0e1c\u0e39\u0e49\u0e27\u0e32\u0e07\u0e1a\u0e34\u0e25 / Billing Person</strong>\n        </div>\n        <div class=\"col-xs-1\"></div>\n    </div>\n    <div class=\"row section-break\">\n        <div class=\"col-xs-1\"></div>\n        <div class=\"col-xs-4 text-center\">\n            <br>\n            <strong>\u0e27\u0e31\u0e19\u0e17\u0e35\u0e48 / Date ___________________________</strong>\n        </div>\n        <div class=\"col-xs-2\"></div>\n        <div class=\"col-xs-4 text-center\">\n            <br>\n            <strong>\u0e27\u0e31\u0e19\u0e17\u0e35\u0e48 / Date ___________________________</strong>\n        </div>\n        <div class=\"col-xs-1\"></div>\n    </div>\n    <div class=\"row section-break\">\n        <div class=\"col-xs-1\"></div>\n        <div class=\"col-xs-4 text-center\">\n            <br>\n            <strong>Payment Date _______________________</strong>\n        </div>\n        <div class=\"col-xs-2\"></div>\n        <div class=\"col-xs-4 text-center\">\n            <br>\n            <strong>Contact No. 053 53 7661 Ext. 113</strong>\n        </div>\n        <div class=\"col-xs-1\"></div>\n    </div>\n    <div class=\"row section-break\">\n        <div class=\"col-xs-1\"></div>\n        <div class=\"col-xs-4 text-center\">\n            <br>\n            <strong>Contact No. _________________________</strong>\n        </div>\n        <div class=\"col-xs-2\"></div>\n        <div class=\"col-xs-4 text-center\">\n            <br>\n            <strong>Email nualanong-r@lplastpack.com</strong>\n        </div>\n        <div class=\"col-xs-1\"></div>\n    </div>\n</div>",
