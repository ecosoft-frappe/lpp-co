 "css": "body {\n    padding: 3mm 0 3mm !important;\n}\n.print-format {\n    line-height: 1.5;\n    margin-left: 5mm;\n    margin-right: 5mm;\n    font-family: \"NotoSansThaiLooped\";\n}\n.print-format, .print-format td, .print-format th {\n    font-size: 12px !important;\n    color: #000000 !important;\n}\n.print-format-preview{\n    padding: 5mm;\n}\n.logo {\n    margin-top: 10px;\n    width: 100px;\n}\n.title {\n    font-size: 18px;\n    font-family: \"NotoSansThai\";\n}\n.title-sales-billing {\n    font-size: 28px !important;\n    padding-top: 30px;\n}\ntable {\n    width: 100%;\n}\n.table-head {\n    border-top: 1px solid #6D6E71;\n    border-bottom: 1px solid #6D6E71;\n}\n.margin-cust-detail {\n    margin-top: 10px;\n}\n.line-break {\n    border-top: 1px solid #6D6E71;\n}\n.col-1 {\n    width: 5%;\n}\n.col-2, .col-3, .col-4 {\n    width: 24%;\n}\n.col-5 {\n    width: 23%;\n}\n.page-break-inside {\n    page-break-inside: avoid;\n}\n.color-thin{\n    color: #6D6E71 !important;\n}",
